.labelContainer {
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
  background-color: #fff;
}

.label {
  display: inline-block;
  font-size: 28rpx;
  background: #e8e8ed;
  border-radius: 10rpx;
  padding: 10rpx 16rpx;
  margin: 20rpx 20rpx 0 20rpx;
}

.joinBtn {
  font-size: 36rpx;
  padding-right: 20rpx;
  color: #0076cb;
}

.weui-navbar__item {
  background-color: #fff;
}

.session_content {
  padding-top: 20rpx;
  background-color: #fff;
}

.myRanking {
  margin-bottom: 1rem;
  background-color: #fff;
}

.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 68rpx;
  height: 68rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.usermotto {
  margin-top: 200px;
}

.weui-cell__ft {
  color: #fdbc40;
  font-size: 1.5rem;
}

.eventItem {
  background-color: #fff;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.session_detail {
  margin-left: 20rpx;
  display: flex;
  align-items: center;
  justify-content: flex-start;
}

.leftContent {
  margin-left: 40rpx;
}

.session_topic {
  margin-left: 20rpx;
}

.session_topic text{
  overflow: hidden;
  word-break: break-all;
  color: #333;
  line-height: 1.25rem;
}

.session_like image {
  width: 40rpx;
  height: 40rpx;
}

.leftContent {
  margin: 15 rpx;
}

.speaker_in_rank {
  font-size: 0.75rem;
}

.session_like {
  display: flex;
  align-items: center;
  margin-right: 10rpx;
}

.title_in_rank text {
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  word-break: break-all;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  color: rgb(0, 0, 0);
  line-height: 1.25rem;
}
